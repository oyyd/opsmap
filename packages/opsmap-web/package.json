{
  "name": "opsmap-web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cpasset": "cp -rf ./node_modules/semantic-ui-css build",
    "build": "npm run cpasset && webpack --config webpack.prod.js",
    "build:dev": "npm run cpasset && webpack --config webpack.dev.js",
    "dev": "npm run cpasset && webpack --config webpack.dev.js -w"
  },
  "author": "oyyd <oyydoibh@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-flowtype-errors": "^3.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "flow-bin": "^0.45.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "semantic-ui-css": "^2.2.10",
    "semantic-ui-react": "^0.68.3",
    "style-loader": "^0.17.0",
    "webpack": "^2.5.1",
    "whatwg-fetch": "^2.0.3"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "plugin:flowtype/recommended"
    ],
    "plugins": [
      "flowtype-errors",
      "flowtype"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "flowtype-errors/show-errors": 2,
      "react/jsx-filename-extension": 0,
      "react/prefer-stateless-function": 0,
      "import/no-extraneous-dependencies": 0
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "flow"
    ],
    "plugins": [
      "transform-async-to-generator"
    ]
  }
}